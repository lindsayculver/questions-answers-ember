<li>Question from {{question.author}}: {{question.content}}</li>

  {{#if isNotesShowing}}
    <p>{{question.notes}}</p>
  {{else}}
    <button {{action 'notesShow'}}>Show Notes</button>
  {{/if}}
  {{update-question question=question update="update"}}
    <button {{action 'delete' question}}>Delete</button>


{{yield}}
